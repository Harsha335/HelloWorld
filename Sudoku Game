    boolean solve()
    {
        for(int i=0;i<9;i++)
        {
            for(int j=0;j<9;j++)
            {
                if(a[i][j]==0)
                {
                    for(int num=1;num<=9;num++)
                    {
                        if(isOk(num,i,j))
                        {
                            a[i][j]=num;
                            if(solve())
                                return true;
                            else
                                a[i][j]=0;
                        }
                    }
                    return false;
                }
            }
        }
        return true;
    }
    boolean isNumInRow(int num,int row)
    {
        for(int i=0;i<9;i++)
        {
            if(num==a[row][i])
                return true;
        }
        return false;
    }
    boolean isNumInColumn(int num,int column)
    {
        for(int i=0;i<9;i++)
        {
            if(num==a[i][column])
                return true;
        }
        return false;
    }
    boolean isNumInBox(int num,int row,int column)
    {
        int localRow=row-row%3;
        int localColumn=column-column%3;
        for(int i=localRow;i<localRow+3;i++)
        {
            for(int j=localColumn;j<localColumn+3;j++)
            {
                if(a[i][j]==num)
                    return true;
            }
        }
        return false;
    }
    boolean isOk(int num,int row,int column)
    {
        return (!isNumInRow(num,row)&&!isNumInColumn(num,column)&&!isNumInBox(num,row,column));
    }
    void print()
    {
        for (int i=0;i<9;i++)
        {
            if(i%3==0&&i!=0)
            {
                System.out.println("- - - - - - - - - - - -");
            }
            for(int j=0;j<9;j++)
            {
                if(j%3==0&&j!=0)
                {
                    System.out.print(" | ");
                }
                System.out.print(a[i][j]+" ");
            }
            System.out.print("\n");
        }
    }
    public static void main(String args[])
    {
        //System.out.println("Enter elementes by row wise : ");
        Sudoku obj=new Sudoku();
        System.out.println("Entered elementes : ");
        obj.input();
        obj.print();
        if(obj.solve())
        {
            System.out.println("\nSuccessful!!!! :)\n");
            obj.print();
        }
        else
        {
            System.out.println("Sorry :(");
        }
    }
}
