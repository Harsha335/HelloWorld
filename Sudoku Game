import java.util.*;
class Sudoku
{
    public int a[][];
    Sudoku()
    {
        a=new int[9][9];
    }
    void input()
    {
        Scanner s=new Scanner(System.in);
        for(int i=0;i<9;i++)
        {
            System.out.println("Enter "+(i+1)+" Row : ");
            for(int j=0;j<9;j++)
            {
                a[i][j]=s.nextInt();
            }
        }
    }
    /*void input()
